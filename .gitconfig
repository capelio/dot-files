[core]
  pager = less -x1,5
  editor = vim
[push]
  default = upstream
[user]
  name = C. Corey Capel
[diff]
  color = auto
[alias]
  br = branch
  ci = commit
  co = checkout
  st = status
  pr = pull --rebase
  sm = submodule
  logs = "log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%C(bold blue)<%an>%Creset' --abbrev-commit"

